{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .js,.ts,.vue",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage=false --passWithNoTests"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@pinia/nuxt": "^0.11.2",
    "axios": "^1.11.0",
    "js-cookie": "^3.0.5",
    "nuxt": "^4.0.3",
    "pinia": "^3.0.3",
    "shadcn-vue": "^2.2.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/jest": "^30.0.0",
    "@types/yup": "^0.32.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.4.0",
    "husky": "^9.1.7",
    "jest": "^30.1.1",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.12",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.1",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.3",
    "vue-eslint-parser": "^10.2.0"
  }
}
